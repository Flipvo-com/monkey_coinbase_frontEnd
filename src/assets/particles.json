{
  "background": {
    "color": "#1c1b22"
  },
  "fullScreen": {
    "enable": true,
    "zIndex": -10
  },
  "interactivity": {
    "events": {
      "onClick": {
        "enable": true,
        "mode": "grab"
      },
      "onHover": {
        "enable": true,
        "mode": "grab"
      }
    },
    "modes": {
      "push": {
        "quantity": 10
      },
      "grab": {
        "distance": 200,
        "links": {
          "blink": false,
          "consent": false,
          "opacity": 1
        }
      }
    }
  },
  "particles": {
    "links": {
      "blink": true,
      "color": {
        "value": "#ffffff"
      },
      "consent": false,
      "distance": 150,
      "enable": true,
      "frequency": 1,
      "opacity": 0.4,
      "shadow": {
        "blur": 5,
        "color": {
          "value": "#000"
        },
        "enable": false
      },
      "triangles": {
        "enable": false,
        "frequency": 1
      },
      "width": 1,
      "warp": false
    },
    "move": {
      "enable": true,
      "speed": 4,
      "angle": {
        "offset": 12,
        "value": 40
      },
      "direction": "top",
      "out_mode": {
        "default": "out",
        "bottom": "out",
        "left": "out",
        "right": "out",
        "top": "out"
      },
        "random": true
    },
    "number": {
      "value": 100
    },
    "opacity": {
      "value": {
        "min": 0.3,
        "max": 0.7
      }
    },
    "size": {
      "value": {
        "min": 1,
        "max": 3
      }
    }
  }
}
